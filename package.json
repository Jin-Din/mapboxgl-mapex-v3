    "animate.css": "^4.1.1",
    "axios": "^1.4.0",
    "element-plus": "^2.3.7",
    "less": "^4.1.3",
    "rollup-obfuscator": "^3.0.2",
    "typescript": "^5.0.2",
    "unplugin-auto-import": "^0.15.1",
    "unplugin-vue-components": "^0.24.1",
    "vite": "^4.3.9",
    "vite-plugin-dts": "^2.3.0",
    "vite-plugin-javascript-obfuscator": "^2.0.0",
